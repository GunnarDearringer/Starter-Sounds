        } else {
            feedbackFormContent.style['display'] = 'none';
            signUpContainer.style['display'] = 'block';

            var authPortalLink = getAuthenticationPageURL(asin, hostname);
            $("#signUpForm").one("click", function () {
                window.location = encodeURI(authPortalLink);
            });
